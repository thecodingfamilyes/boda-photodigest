{
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"sourceType": "module"
	},
	"env": {
		"browser": false,
		"node": true,
		"es6": true,
		"jasmine": true
	},
	"rules": {
		"no-extra-semi": 1,
		"no-unused-vars": [
			2,
			{
				"args": "none"
			}
		],
		"no-console": 0,
		"semi": [
			2,
			"always"
		]
	}
}
